<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <listener>
  	<listener-class>com.web.listener.RequestListener</listener-class>
  </listener>
  
  <filter>
  	<filter-name>FilterEconding</filter-name>
  	<filter-class>com.web.filter.FilterEconding</filter-class>
  	<init-param>
 		<param-name>Encoding</param-name>
 		<param-value>编码</param-value>
 	</init-param>  	
  </filter>
  
  <filter-mapping>
  	<filter-name>FilterEconding</filter-name>
  	<url-pattern>/servlet/*</url-pattern>
  </filter-mapping>
  
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.web.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>StaffServlet</servlet-name>
    <servlet-class>com.web.servlet.StaffServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SetTablesServlet</servlet-name>
    <servlet-class>com.web.servlet.SetTablesServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RawMaterialServlet</servlet-name>
    <servlet-class>com.web.servlet.RawMaterialServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>EntryOrderServlet</servlet-name>
    <servlet-class>com.web.servlet.EntryOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>BaseInfoServlet</servlet-name>
    <servlet-class>com.web.servlet.BaseInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FindOrderServlet</servlet-name>
    <servlet-class>com.web.servlet.FindOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FindRepertoryServlet</servlet-name>
    <servlet-class>com.web.servlet.FindRepertoryServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MemberServlet</servlet-name>
    <servlet-class>com.web.servlet.MemberServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>OrderListServlet</servlet-name>
    <servlet-class>com.web.servlet.OrderListServlet</servlet-class>
  </servlet>










  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/servlet/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StaffServlet</servlet-name>
    <url-pattern>/servlet/StaffServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SetTablesServlet</servlet-name>
    <url-pattern>/servlet/SetTablesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RawMaterialServlet</servlet-name>
    <url-pattern>/servlet/RawMaterialServlet</url-pattern>
  </servlet-mapping>
